@CHARSET "ISO-8859-1";
*{margin: 0px;padding: 0px;}
body{font-family: Tahoma,verdana;font-size: 12px;padding: 0px;margin: 0px;border: 0px solid #111;}
.textbox_small,.textbox_medium,.textbox,.submit{border: 1px solid #989898;background-color:#fff;padding:4px;font-size: 12px;}
.textbox{width: 250px}
.textbox_medium{width: 130px}
.textbox_small{width: 80px}
.submit,.button{cursor:pointer;background-color:#37badc;color:#196db6; font-weight: normal;padding: 2px 15px 2px 15px ;color: #fff;border: 1px solid #8dd5ff;text-transform:capitalize;}
.submit{background-color: #5bb14d;border:2px solid #d9fd7d}
a{text-decoration: none;border: 0px;color:#496db6;}
img{border: 0px;}
h2{padding-bottom: 0px;padding-top: 10px;margin: 4px;font-size: 14px;}
hr{border: 0px;border-top: 1px solid #cdcdcd;}
table.spreadsheet{ width:98%;margin:10px 0; border:#bababa 1px solid; }
table.spreadsheet th{border-right:1px solid #bababa; background:#dedede;color:#434343; font-weight:normal; font-size:12px; padding:2px 2px;}
table.spreadsheet td{ border-right:1px solid #cdcdcd;border-top:#cdcdcd 1px solid; padding:3px 3px; }
table.spreadsheet tr:hover{ background-color: #efefef; }
.comments{color:#646464;font-size: 10px}
