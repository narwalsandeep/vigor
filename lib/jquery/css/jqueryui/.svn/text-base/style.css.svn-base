.zend_form{background:#fff;width:600px;padding:5px;padding-left:30px;border-top:1px solid #cdcdcd;}
.zend_form dd{margin-bottom: 10px;}
.required:after{content:'* ';color:#cc2233;font-size:12px}
.zend_form ul.errors li{list-style-type: none;color:#cc2211;font-size: 11px;}

/* ####################### */
.link_delete{font-size: 11px;font-weight:normal;color: #cc2211;}
.success{margin:8px;width:500px;color:green;padding:4px;background-color: #e2edd4;border-left:2px solid green}
.error{margin:8px;width:500px;color:red;padding:4px;background-color: #f8f0f0;border-left:2px solid #cc3322}
.info{margin:8px;width:500px;color:#fff;padding:4px;background-color: #e6b842;border-left:2px solid #cc3322}
.highlight{font-size: 11px;color: #979797}

